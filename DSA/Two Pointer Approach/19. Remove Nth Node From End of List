class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        
        ListNode dummy = new ListNode(0);

        dummy.next = head;
        int len=0;

        ListNode temp = head;

        while(temp !=null){
             len++;
             temp= temp.next;
        }

        int del = len-n;

        ListNode prev = dummy;
        
       for(int i =0;i<del;i++){
     prev= prev.next;
    }

     prev.next= prev.next.next;
    
    return dummy.next;
    }
}
